{
  "name": "@speakeasy-api/docs-md-e2e",
  "version": "0.2.34",
  "private": true,
  "description": "End-to-end integration tests for DocsMD",
  "type": "module",
  "scripts": {
    "test": "playwright test",
    "test:docusaurus": "playwright test --project=docusaurus-mistral",
    "test:nextra": "playwright test --project=nextra-mistral",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "prettier": "prettier --config ../../prettier.config.mjs --ignore-path ../../.gitignore \"**/*.{js,jsx,mjs,ts,tsx,mts}\"",
    "format": "npm run prettier -- --write",
    "check-formatting": "npm run prettier -- --check",
    "lint": "eslint \"**/*.{js,jsx,mjs,ts,tsx,mts}\"",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@playwright/test": "1.56.0",
    "playwright": "1.56.0"
  },
  "devDependencies": {
    "@types/node": "^24.7.2",
    "typescript": "^5.9.3"
  }
}
