      "default": "./dist/runtime/docusaurus.css"
    },
    "./nextra.css": {
      "default": "./dist/runtime/nextra.css"
    },
    "./react": {
      "types": "./dist/runtime/react.d.ts",
      "default": "./dist/runtime/react.js"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "",
