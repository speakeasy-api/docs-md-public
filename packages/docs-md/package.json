      "types": "./dist/runtime/docusaurus.d.ts",
      "default": "./dist/runtime/docusaurus.js"
    },
    "./nextra": {
      "types": "./dist/runtime/nextra.d.ts",
      "default": "./dist/runtime/nextra.js"
    },
