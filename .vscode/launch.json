  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Mistral",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/packages/chunk-generator/src/cli/cli.ts",
      "runtimeArgs": ["--experimental-strip-types"],
      "args": [
        "--spec",
        "${workspaceFolder}/../mistral-docs/openapi.yaml",
        "-p",
        "${workspaceFolder}/../mistral-docs/docs/api",
        "-c",
        "${workspaceFolder}/../mistral-docs/src/components/speakeasy",
        "-f",
        "docusaurus"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Redis",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/packages/chunk-generator/src/cli/cli.ts",
